{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^Jnpf(.*)": "@/components/Jnpf/$1/index.vue",
			"^kex-(.*)": "@/components/kex/$1/index.vue"
		}
	},
	"pages": [{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "工作台",
				"navigationBarTextStyle": "white"
			}
		},
		{
			"path": "pages/index/home",
			"style": {
				"navigationBarTitleText": "首页"
			}
		},
		{
			"path": "pages/index/list",
			"style": {
				"navigationBarTitleText": "商品"
			}
		},
		{
			"path": "pages/home/search",
			"style": {
				"navigationBarTitleText": "搜索"
			}
		},
		{
			"path": "pages/index/complate",
			"style": {
				"navigationBarTitleText": "结算"
			}
		},
		{
			"path": "pages/index/my",
			"style": {
				"navigationBarTitleText": "我的"
			}
		}, {
			"path": "uni_modules/uni-id-pages/pages/login/login-withoutpwd",
			"style": {
				"navigationBarTitleText": "微信登录"
			}
		},
		{
			"path": "uni_modules/uni-id-pages/pages/login/login-withpwd",
			"style": {
				"navigationBarTitleText": "账号登录"
			}
		},
		{
			"path": "uni_modules/uni-id-pages/pages/register/register",
			"style": {
				"navigationBarTitleText": "账号注册"
			}
		},
		{
			"path": "uni_modules/uni-id-pages/pages/userinfo/change_pwd/change_pwd",
			"style": {
				"navigationBarTitleText": "修改密码"
			}
		},

		{
			"path": "uni_modules/uni-id-pages/pages/common/PrivacyPolicy/PrivacyPolicy",
			"style": {
				"navigationBarTitleText": "用户服务协议"
			}
		},
		{
			"path": "uni_modules/uni-id-pages/pages/common/UserAgreement/UserAgreement",
			"style": {
				"navigationBarTitleText": "隐私政策条款"
			}
		},
		{
			"path": "uni_modules/uni-id-pages/pages/userinfo/deactivate/deactivate",
			"style": {
				"navigationBarTitleText": "注销账号"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "扫码识别盘点录入助手",
		"navigationBarBackgroundColor": "#00aaff",
		"backgroundColor": "#00aaff"
	},
	"tabBar": {
		"color": "#9799a5",
		"selectedColor": "#28B389",
		"list": [{
				"text": "工作台",
				"pagePath": "pages/index/index",
				"iconPath": "static/tabBar/index.png",
				"selectedIconPath": "static/tabBar/indexH.png"
			}, {
				"text": "结算",
				"pagePath": "pages/index/complate",
				"iconPath": "static/tabBar/checkout.png",
				"selectedIconPath": "static/tabBar/checkout-active.png"
			},
			{
				"text": "商品",
				"pagePath": "pages/index/list",
				"iconPath": "static/tabBar/data.png",
				"selectedIconPath": "/static/tabBar/data-active.png"
			},
			{
				"text": "我的",
				"pagePath": "pages/index/my",
				"iconPath": "static/tabBar/my.png",
				"selectedIconPath": "static/tabBar/my-active.png"
			}
		]
	},
	"uniIdRouter": {
		"loginPage": "uni_modules/uni-id-pages/pages/login/login-withoutpwd",
		"needLogin": [
			"pages/subpack/usermsg/usermsg",
			"pages/subpack/record/record",
			"pages/subpack/analysis/analysis"
		]
	},
	"subPackages": [{
		"root": "pages/subpack",
		"pages": [{
			"path": "explanation/explanation",
			"style": {
				"navigationBarTitleText": "导入说明",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "analysis/analysis",
			"style": {
				"navigationBarTitleText": "数据分析",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "record/record",
			"style": {
				"navigationBarTitleText": "导出记录",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "usermsg/usermsg",
			"style": {
				"navigationBarTitleText": "我的消息",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "pay/pay",
			"style": {
				"navigationBarTitleText": "支持作者",
				"enablePullDownRefresh": false
			}
		}]
	}]
}